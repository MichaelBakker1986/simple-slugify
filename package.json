{
  "name": "slug-web-friendly",
  "version": "0.2.0",
  "description": "Out of the box slugify method",
  "main": "./dist/bundle-umd.js",
  "module": "./dist/bundle.mjs",
  "scripts": {
    "build:esm": "rollup index.mjs --file ./dist/bundle-umd.js --f umd --name \"slugify\"",
    "build:cjs": "rollup index.mjs --file ./dist/bundle.cjs --f cjs",
    "build:es": "rollup index.mjs --file ./dist/bundle.mjs --f es",
    "build:browser": "rollup index.mjs --file ./dist/bundle.js --f iife --name \"slugify\"",
    "prepack": "npm run build:esm && npm run build:cjs && npm run build:es && npm run build:browser",
    "test": "./test/test.mjs"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/MichaelBakker1986/slug-web-friendly.git"
  },
  "release": {
    "branches": [
      "master"
    ]
  },
  "publishConfig": {
    "access": "public"
  },
  "keywords": [
    "slugify",
    "slug",
    "slugger",
    "slugify string",
    "slugify string to url"
  ],
  "author": "Michael Bakker",
  "license": "NONE",
  "exports": {
    "slugify": {
      "import": {
        "default": "./dist/bundle.mjs"
      },
      "require": {
        "default": "./dist/bundle-umd.js"
      }
    },
    "default": {
      "import": {
        "default": "./dist/bundle.mjs"
      },
      "require": {
        "default": "./dist/bundle-umd.js"
      }
    }
  },
  "files": [
    "dist/*"
  ],
  "bugs": {
    "url": "https://github.com/MichaelBakker1986/slug-web-friendly/issues"
  },
  "homepage": "https://github.com/MichaelBakker1986/slug-web-friendly#readme"
}
